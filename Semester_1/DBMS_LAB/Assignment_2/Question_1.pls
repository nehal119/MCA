/* Create Table EMP  */
CREATE TABLE EMP (
    ENAME VARCHAR2(20),
    DEPT_NAME VARCHAR2(20),
    DESIGNATION VARCHAR2(20),
    SALARY NUMBER(8),
    DATE_OF_JOIN DATE DEFAULT SYSDATE
);

