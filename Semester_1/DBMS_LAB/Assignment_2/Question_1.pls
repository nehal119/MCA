SELECT * FROM EMP;

/* (a) Find all the ENAMEâ€™s whose salary is < Rs.20000   */
SELECT ENAME FROM EMP WHERE SALARY < 20000;

/* (b) Find all the employees working with SALES Department and with designation MANAGER.  */
SELECT ENAME FROM EMP WHERE DEPT_NAME = 'SALES' AND DESIGNATION = 'MANAGER';

/* (c) Find all employees whose name starts with S  */
SELECT ENAME FROM EMP WHERE ENAME LIKE 'S%';

/* (d) Find total number of employees who work with RESEARCH department  */
SELECT COUNT(ENAME) "No. of employees working with RESEARCH department" from EMP WHERE DEPT_NAME = 'RESEARCH';

/* (e) Find all the employees who joined after Jan 1, 2010.  */
SELECT ENAME FROM EMP WHERE DATE_OF_JOIN > TO_DATE('Jan 1, 2010', 'mm dd, yyyy');
SELECT ENAME FROM EMP WHERE DATE_OF_JOIN > '01jan2010';

/* (f)Count number of employees whose salary is between Rs.8000 and Rs.12500.  */
SELECT COUNT(ENAME) "No. of employees with salary between Rs.8000 and Rs.12500" from EMP WHERE SALARY BETWEEN 8000 AND 12500;

/* (g)Sort the supplier table by ENAME.  */
SELECT * FROM EMP ORDER BY ENAME ASC;

/* (h)Find the employees whose designation is SALESMAN and joined after 1st Aug, 1990.  */
SELECT ENAME FROM EMP WHERE DESIGNATION = 'SALESMAN' AND DATE_OF_JOIN > '01aug1990';

/* (i)Find all the employees whose designation is CLERK.  */
SELECT ENAME FROM EMP WHERE DESIGNATION = 'CLERK';

/* (j)Count number of SALESMAN in SALES department.  */
SELECT COUNT(ENAME) "No. of SALESMAN in SALES department" from EMP WHERE DEPT_NAME = 'SALES' AND DESIGNATION = 'SALESMAN';

/* (k)Count all the number of employees who are working with the company.  */
SELECT COUNT(ENAME) "No. of employees who are working with the company" from EMP;

/* (l)Find the employees joined between 1st Jan, 1997 and 31st Dec, 2010.  */
SELECT ENAME FROM EMP WHERE DATE_OF_JOIN > '01jan1997' AND DATE_OF_JOIN < '31dec2010';

/* (m)Sort the table by the SALARY, descending order.  */
SELECT * FROM EMP ORDER BY SALARY DESC;